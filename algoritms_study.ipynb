    "n = 1260\r\n",
    "count = 0\r\n",
    "\r\n",
    "#큰 단위의 화폐부터 차례대로 확인\r\n",
    "list = [500, 100, 50, 10]\r\n",
    "\r\n",
    "for coin in list:\r\n",
    "    count += n // coin #해당 화폐로 거슬러 줄 수 있는 동전의 개수 세기. // 연산자의 의미는 정수 부분 몫 값을 취하는 것을 의미함.\r\n",
    "    n %= coin\r\n",
    "\r\n",
    "print(count)"
